Main.Size = UDim2.new(0.305114597, 0, 0.338290006, 0)
Main.ZIndex = 3
Main.Style = Enum.FrameStyle.ChatBlue

Title.Name = "Title"
Title.Parent = Main
Title.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
