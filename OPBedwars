-- Scripts:

local function CUXCGYR_fake_script() -- ScreenGui.Script 
	local script = Instance.new('Script', ScreenGui)

	Main = script.Parent.Main
	Main.Draggable = true
	Main.Active = true
	Main.Selectable = true
end
coroutine.wrap(CUXCGYR_fake_script)()
